<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mzl0101.tree.mapper.SysTreeNodeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.mzl0101.tree.entity.SysTreeNode">
        <id column="node_id" property="nodeId" />
        <result column="node_name" property="nodeName" />
        <result column="node_parent_id" property="nodeParentId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        node_id, node_name, node_parent_id
    </sql>

</mapper>
